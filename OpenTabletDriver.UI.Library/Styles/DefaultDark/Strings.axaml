<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <x:String x:Key="output_mode_combobox_help">An output mode defines how the input from a tablet is presented to the OS.</x:String>
    <x:String x:Key="output_mode_display_x_offset">The horizontal displacement of the top-left corner of the mapped area</x:String>
    <x:String x:Key="output_mode_display_y_offset">The vertical displacement of the top-left corner of the mapped display area</x:String>
    <x:String x:Key="output_mode_display_width">The width of the mapped display area</x:String>
