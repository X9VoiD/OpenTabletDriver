<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="BackgroundColor">#202020</Color>
    <Color x:Key="ContainerColor">#323232</Color>
    <SolidColorBrush x:Key="BackgroundColorBrush" Color="{DynamicResource BackgroundColor}" />
    <SolidColorBrush x:Key="ContainerColorBrush" Color="{DynamicResource ContainerColor}" />
</ResourceDictionary>
